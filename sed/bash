echo ' ================================== '
sed '/Peter/' table.txt
echo ' ================================== '
sed '/Peter/p' table.txt
echo ' ================================== '
sed -n '/Peter/p' table.txt
echo ' ================================== '
sed -n '3p' table.txt
echo ' ================================== '
sed '3p' table.txt
echo ' ================================== '
sed -n '2,7p' table.txt
